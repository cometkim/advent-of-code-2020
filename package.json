{
  "name": "advent-of-code-2020",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "pesy build",
    "release": {
      "rewritePrefix": true,
      "bin": [ "AdventOfCode2020App.exe" ]
    }
  },
  "buildDirs": {
    "src/lib": {
      "imports": [
        "Lwt = require('lwt')",
        "Fp = require('@reason-native/fp')",
        "Fs = require('@reason-native/fs/lib')",
        "Console = require('@reason-native/console/lib')"
      ]
    },
    "src/bin": {
      "imports": [
        "Lib = require('advent-of-code-2020/src/lib')",
        "Inquire = require('@opam/inquire')"
      ],
      "bin": { "AdventOfCode2020App.exe": "App.re" }
    },
    "test/lib": {
      "imports": [ "Rely = require('@reason-native/rely/lib')" ],
      "flags": [ "-linkall", "-g" ]
    },
    "test/bin": {
      "imports": [ "Test = require('advent-of-code-2020/test/lib')" ],
      "bin": { "RunTests.exe": "RunTests.re" }
    }
  },
  "scripts": {
    "start": "esy x AdventOfCode2020App.exe",
    "test": "esy x RunTests.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": "*",
    "@opam/inquire": "0.2.1",
    "@opam/lwt": "*",
    "@pesy/esy-pesy": "0.1.0-alpha.13",
    "@reason-native/console": "^0.1.0",
    "@reason-native/fp": "*",
    "@reason-native/fs": "*",
    "@reason-native/rely": "^3.2.1",
    "ocaml": "~4.10.0"
  },
  "devDependencies": {
    "refmterr": "*",
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam",
    "@opam/merlin": "*"
  }
}
